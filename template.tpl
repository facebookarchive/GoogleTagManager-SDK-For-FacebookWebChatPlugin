___SANDBOXED_JS_FOR_WEB_TEMPLATE___

const injectScript = require('injectScript');
const copyFromWindow = require('copyFromWindow');
const createQueue = require('createQueue');
const langCode = data.langCode;

const chatUrl = 'https://connect.facebook.net/' + langCode + '/sdk/xfbml.customerchat.js';

const dlPush = createQueue('dataLayer');

const cb = type => {
  dlPush({
    event: data.dataLayerEvent,
    fbChatAction: type
  });
};

const loaded = () => {
  const FB = copyFromWindow('FB');
  if (FB) {
    FB.init({
      appId            : data.appId,
      autoLogAppEvents : true,
      xfbml            : true,
      version          : 'v6.0'
    });
    if (data.pluginLoad) FB.Event.subscribe('customerchat.load', () => { cb('pluginLoad'); });
    if (data.pluginShow) FB.Event.subscribe('customerchat.show', () => { cb('pluginShow'); });
    if (data.pluginHide) FB.Event.subscribe('customerchat.hide', () => { cb('pluginHide'); });
    if (data.dialogShow) FB.Event.subscribe('customerchat.dialogShow', () => { cb('dialogShow'); });
    if (data.dialogHide) FB.Event.subscribe('customerchat.dialogHide', () => { cb('dialogHide'); });
  } else {
    data.gtmOnFailure();
  }
  data.gtmOnSuccess();
};

injectScript(chatUrl, loaded, data.gtmOnFailure, 'fbcsdk');


