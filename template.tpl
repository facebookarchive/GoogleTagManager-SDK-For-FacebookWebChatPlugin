- name: Event handlers are added
  code: |-
    const events = ['customerchat.load', 'customerchat.show', 'customerchat.hide', 'customerchat.dialogShow', 'customerchat.dialogHide'];

    mock('copyFromWindow', item => {
      if (item !== 'FB') fail('copyFromWindow called with incorrect key');
      return {
        Event: {
          subscribe: (event, cb) => {
            if (events.indexOf(event) === -1) fail('FB.Event.subscribe called with incorrect event name');
            events.splice(events.indexOf(event), 1);
            cb();
          }
        },
        init: () => {}
      };
    });

    // Call runCode to run the template's code.
    runCode(mockData);

    if (events.length > 0) fail('Some events were not handled: ' + events);

    // Verify that the tag finished successfully.
    assertApi('gtmOnSuccess').wasCalled();
